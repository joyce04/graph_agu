{
  "data_loc": "./dataset/",
  "dataset": "cora",
  "seed": 1,
  "data_split": "public",
  "subset_params": {
    "edge_split": true,
    "train_ratio": 0.01
  },
  "gnn": "graphsage",
  "preepochs": 500,
  "epochs": 150,
  "learning_rate": 0.003,
  "weight_decay": 0.0005,
  "patience": 10,
  "aug_type": "flag_orig",
  "flag_params": {
    "m": 3,
    "step_size": 1e-3,
    "cr": false,
    "biased": false
  }
}